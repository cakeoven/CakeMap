{
  "name": "cakephp/cakephp_cakemap",
  "description": "A CakePHP helper to let u use transparently google maps",
  "type": "cakephp-plugin",
  "license": "MIT",
  "keywords": [
    "helper",
    "google_maps"
  ],
  "homepage": "http://github.com/gmponos/CakeMap",
  "support": {
    "issues": "https://github.com/gmponos/CakeMap/issues"
  },
  "autoload": {
    "psr-4": {
      "CakeMap\\": "src",
      "CakeMap\\Test\\": "tests"
    }
  },
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "^2.6"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "*",
    "phpunit/phpunit": "*"
  },
  "suggest": {
    "apigen/apigen": "Produce Documentations very easily via DocBlocks"
  },
  "prefer-stable": true
}
