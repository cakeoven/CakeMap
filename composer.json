{
  "name": "cakephp/cakephp_cakemap",
  "description": "A CakePHP helper to let u use transparently google maps",
  "type": "helper",
  "keywords": [
    "helper",
    "google_maps"
  ],
  "support": {
    "issues": "https://github.com/gmponos/CakeMap/issues"
  },
  "homepage": "http://github.org/gmponos/CakeMap",
  "license": "MIT",
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "CakeMap\\": "src",
      "CakeMap\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "cakephp/cakephp-codesniffer": "2.*",
    "phpunit/phpunit": "*"
  }
}