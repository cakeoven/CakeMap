{
    "name": "cakeoven/cakemap",
    "description": "A CakePHP helper to let u use transparently google maps",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": [
        "helper",
        "google_maps"
    ],
    "homepage": "http://github.com/cakeoven/CakeMap",
    "support": {
        "issues": "https://github.com/cakeoven/CakeMap/issues"
    },
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "3.*"
    },
    "autoload": {
        "psr-4": {
            "CakeOven\\CakeMap\\": "src",
            "CakeOven\\CakeMap\\Test\\": "tests"
        }
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "*",
        "phpunit/phpunit": "*"
    },
    "suggest": {
        "apigen/apigen": "Produce Documentations very easily via DocBlocks"
    },
    "prefer-stable": true
}
